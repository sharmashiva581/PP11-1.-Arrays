import java.io.*;
import java.util.*;

public class Main {

    public static void main(String[] args) throws Exception {
        // write your code here
          Scanner scn = new Scanner(System.in);
        int n= scn.nextInt();
        int mat[][] = new int[n][n];
        for(int i=0 ; i< n ; i++){
            for(int j=0 ; j< n ; j++){
                mat[i][j] = scn.nextInt();
            }
        }
        int x = scn.nextInt();
        for(int r= 0 ; r< n  ; r++){
            for(int c = 0 ; c< n ; c++){
                if(mat[r][c] == x){
                    System.out.println(r);
                    System.out.println(c);
                    return;
                }
            }
        }
        System.out.println("Not Found");
    }

}
